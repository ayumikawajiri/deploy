<div class="body">
  <div class="breadcrumbs" >
    <%= render_breadcrumbs separator: ' > ' %>
  </div>

  <h1 class="h1-text">アカウント登録</h1>

  <div class="section">
    <div class="form">
      <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>  
        <P><%= devise_error_messages! %></P>

        <div class="field">
          <%= f.label "ユーザーネーム" %>
          <%= f.text_field :username, :size=>"65x1", autofocus: true %>
        </div>

        <div class="field">
          <%= f.label "メールアドレス" %>
          <%= f.email_field :email, :size=>"65x1", autofocus: true %>
        </div>

        <div class="field">
          <%= f.label "パスワード" %>
          <%= f.password_field :password, :size=>"65x1", autocomplete: "off" %>
